        "Command__": [
          "#[($(__FinalBuild) == 1) {",
          "    #[NuGet gnt.raw(\"/t:pack /p:ngin=\\\"$(onupkg)\\\" /p:ngout=\\\"$(odir)\\\"\")]",
          "}]"
        ]
      }
    }
